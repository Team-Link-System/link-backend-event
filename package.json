{
  "name": "link-backend-event",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "clean": "rm -rf node_modules/.cache/hard-source && rm -rf dist",
    "ensure-dist": "mkdir -p dist",
    "dev": "NODE_ENV=development nodemon src/server.ts",
    "build": "tsc && ls -la dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "predocker:build": "npm run ensure-dist",
    "docker:build": "docker build -f Dockerfile -t harbor.jongjong2.site:30443/link-event/link-event:latest .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t harbor.jongjong2.site:30443/link-event/link-event:dev .",
    "docker:push": "docker push harbor.jongjong2.site:30443/link-event/link-event:latest",
    "docker:push:dev": "docker push harbor.jongjong2.site:30443/link-event/link-event:dev",
    "build:dev": "npm run clean && npm run build && if [ -d \"dist\" ]; then npm run docker:build:dev && npm run docker:push:dev; fi",
    "build:prod": "npm run clean && npm run build && if [ -d \"dist\" ]; then npm run docker:build && npm run docker:push; fi"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "croner": "^9.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "ioredis": "^5.5.0",
    "mongoose": "^8.8.3",
    "nats": "^2.28.2",
    "pg": "^8.13.3",
    "pg-promise": "^11.10.2",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
